{
    "family": "start-nginx-reverse-proxy",
    "containerDefinitions": [
        {
            "name": "nginx-reverse-proxy",
            "image": "587191779468.dkr.ecr.us-east-1.amazonaws.com/nginx-reverse-proxy:latest",
            "cpu": 512,
            "portMappings": [
                {
                    "name": "nginx-reverse-proxy-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "nginx-reverse-proxy-443-tcp",
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp",
                    "appProtocol": "https"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": []
   
        }
    ],
    "executionRoleArn": "arn:aws:iam::587191779468:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}